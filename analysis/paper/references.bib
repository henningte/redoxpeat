 pages = {139--145},
 volume = {41},
 number = {1},
 journal = {{Environmental science {\&} technology}},
 doi = {10.1021/es061323j},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/17265939},
 file = {Bauer_2007_electron_transfer_capacities:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Bauer_2007_electron_transfer_capacities.pdf:pdf}
