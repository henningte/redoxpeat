 doi = {10.1038/s41467-018-06050-2},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/30194308},
 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6128871},
 file = {Hodgkins, Richardson et al. 2018 - Tropical peatland carbon storage:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Hodgkins, Richardson et al. 2018 - Tropical peatland carbon storage.pdf:pdf;41467_2018_6050_MOESM1_ESM:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\41467_2018_6050_MOESM1_ESM.pdf:pdf}
