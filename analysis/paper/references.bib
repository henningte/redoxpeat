 file = {http://www.ncbi.nlm.nih.gov/pubmed/31587530},
 file = {Li_et_al-2020-Biotechnology_Progress:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Li_et_al-2020-Biotechnology_Progress.pdf:pdf}
